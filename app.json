{
  "expo": {
    "name": "astronomy-weather-app",
    "slug": "astronomy-weather-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "m",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
